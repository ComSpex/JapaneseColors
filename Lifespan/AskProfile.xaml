<UserControl x:Class="Lifespan.AskProfile"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" >
	<Grid HorizontalAlignment="Center" VerticalAlignment="Center">
		<Grid>
			<DatePicker x:Name="birth" HorizontalAlignment="Left" Margin="0,28,0,0" VerticalAlignment="Top"/>
			<DatePicker x:Name="dead" HorizontalAlignment="Left" Margin="147,28,0,0" VerticalAlignment="Top"/>
			<TextBox x:Name="name" HorizontalAlignment="Left" Height="23" TextWrapping="Wrap" Text="TextBox" VerticalAlignment="Top" Width="249"/>
			<TextBox x:Name="desc" HorizontalAlignment="Left" Height="193" Margin="0,57,0,0" TextWrapping="Wrap" Text="TextBox" VerticalAlignment="Top" Width="249" ScrollViewer.CanContentScroll="True"/>
		</Grid>
	</Grid>
</UserControl>
